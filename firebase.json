{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint",
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ]
  },
  "hosting": {
    "public": "build",
    "rewrites": [
      {
        "source": "/iapApp/**",
        "function": "iapApp"
      },
      {
        "source": "/joinSlack",
        "function": "joinSlack"
      },
      {
        "source": "/joinWeCount",
        "function": "joinWeCount"
      },
      {
        "source": "/joinRNSeoul",
        "function": "joinRNSeoul"
      },
      {
        "source": "/joinPrismaKorea",
        "function": "joinPrismaKorea"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "ignore": [
      "**/.*",
      "**/node_modules/**",
      "firebase.json"
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}